<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>

<?import java.net.URL?>
<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.uninhibited.controllers.AllCarsController"
      stylesheets="@styles/cars.css"
      style="-fx-padding: 10; -fx-font-size: 14pt;">
    <ListView fx:id="listView" prefWidth="800" prefHeight="600" styleClass="car-list"/>

    <stylesheets>
        <URL value="@styles/cars.css" />
    </stylesheets>
</VBox>